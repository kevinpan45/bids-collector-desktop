{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "dialog:allow-open",
    "dialog:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-create",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "fs:default",
    "fs:scope-appdata",
    "fs:scope-appdata-recursive"
  ]
}
